        <thead>
            <th>Name</th>
            <th>Email</th>
            <th>Password</th>
            <th></th>
            <th></th>
        </thead>

            <% Users.forEach((user) => { %>
                <tr>
                <td> <%= user.name %> </td> 
                <td> <%= user.email %> </td>
